[alias]
    st = status
    sts = status --short
    ci = commit
    cia = commit --amend
    co = checkout
    br = branch
    puld = pull origin dev
    pulm = pull origin master
    pulld = pull origin dev
    pullm = pull origin master
    pullr = pull origin release
    pulr = pull origin release
    pusd = push origin dev
    pusm = push origin master
    pushd = push origin dev
    pushm = push origin master
    pushr = push origin release
    pusr = push origin release
    fd = pull origin dev       # fd - from dev
    td = push origin dev       # td - to dev
    fm = pull origin master    # fm - from master
    tm = push origin master    # tm - to master
    fr = pull origin release   # fr - from release
    tr = push origin release   # tr - to release
    gob = checkout -b
    god = checkout dev
    gom = checkout master
    gor = checkout release
    sh = stash
    sha = stash apply
    shp = stash pop
    mg = merge
    mx = merge -Xignore-all-space
    mema = merge master
    plo = pull origin
    pso = push origin
    df = diff
    dfs = diff --staged
    dfis = diff --ignore-space-change
    ad = add
    a = add
    addp = add --patch
    ap = add --patch
    au = add -u .
    ns = log --name-status
    lg = log --graph
    lgns = log --graph --name-status
    showis = show --ignore-space-change
    rbi = rebase -i
    rebm = rebase master
    rema = rebase master
    rh = reset HEAD~
    m1 = reset HEAD~  # m1 - minus 1
    cp = cherry-pick
    cpa = cherry-pick --abort
    cpc = cherry-pick --continue
[color]
    ui = true
[credential]
    helper = cache --timeout=86400
[user]
    email = example@mail.ru
    name = u-mulder
